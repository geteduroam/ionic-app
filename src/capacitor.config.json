{
  "appId": "com.emergya.geteduroam",
  "appName": "geteduroam",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "cordova": {
    "preferences": {
      "ScrollEnabled": "false",
      "android-minSdkVersion": "19",
      "BackupWebStorage": "none",
      "SplashShowOnlyFirstTime": "false",
      "SplashScreen": "false"
    }
  },
  "plugins": {
    "SplashScreen": {
      "launchAutoHide": true,
      "showSpinner": false
    }
  }
}
